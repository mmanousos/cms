<form action='/users/signin' method='post'>
  <label for='username'>Username: </label>
  <input type='text' name='username' id='username' value='<%= params[:username] %>'><br>
  <label for='password'>Password: </label>
  <input type='password' name='password' id='password' value='<%= params[:password] %>'><br>
  <button type='submit' id='sign_in' name='signin'>Sign In</button>
</form>
<a href='/'><button class='cancel'>Cancel</button></a>
